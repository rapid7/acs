{
  "presets": [
    ["env", {
      "targets": {
        "node": "6"
      }
    }],
    "stage-2"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false
    }],
    "transform-decorators-legacy",
    "add-module-exports",
    "transform-async-generator-functions",
    "transform-object-rest-spread"
  ]
}
